  use_frameworks!
  pod 'RakutenAdvertisingAttribution', :git => 'https://github.com/Rakuten-Advertising-Developers/RakutenAdvertisingAttribution-iOS-SDK.git', :branch => 'feature/Fingerprint'
  
  pod 'Firebase/Firestore'
  pod 'Firebase/Auth'
  
end
